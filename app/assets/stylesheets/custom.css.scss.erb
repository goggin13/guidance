

.nav {
  li {
    list-style-type: none;
    float: left;
    clear: none;
    border-radius: 0px 0px 5px 5px;
    padding: 10px;
    background-color: #5E8DA5;
    a, a:hover, a:visited {
      color: white;
      text-decoration: none;
      font-size: 1.3em;
    }
  }
}

#header {
  background-color: #00CEAD;
  color: white;
  font-size: 2em;
  padding: 10px;
}

#chat_window_container {
  background: none repeat scroll 0 0 white;
  border: thin solid black;
  display: none;
  float: right;
  padding: 10px;
  width: 258px;
}

#home_page_banner {
  background-image: url(<%= asset_path 'group_shot.jpg' %>);
  color: white;
  font-size: 2em;
  height: 203px;
  line-height: 1.5em;
  padding: 30px 0 0 30px;
  width: 870px;  
}

